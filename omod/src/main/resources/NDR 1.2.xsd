			<xs:element name="VisitID" type="StringType" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						The unique Visit
						Identifier
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VisitDate" type="xs:date" minOccurs="1"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Date of Visit
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DurationOnArt" type="xs:int" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Duration (in Months)on
						ART this visit
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Weight" type="xs:int" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						The Patient's Weight in
						(kg) this visit
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ChildHeight" type="xs:int" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						The Patient's Height in
						(cm) if child this visit
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BloodPressure" type="StringType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Blood Pressure (mmHg)
						Adults Only this visit
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EDDandPMTCTLink" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the EDD and PMTCT Link of the patient this visit
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="P" />
						<xs:enumeration value="PMTCT" />
						<xs:enumeration value="NP" />
						<xs:enumeration value="NK" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PatientFamilyPlanningCode" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the use of Family Planning by patient this visit
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="FP" />
						<xs:enumeration value="NOFP" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PatientFamilyPlanningMethodCode" type="CodeType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the Family Planning Method used by patient this
						visit
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FunctionalStatus" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the Functional Status of the patient this visit
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="W" />
						<xs:enumeration value="A" />
						<xs:enumeration value="B" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="WHOClinicalStage" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the WHO Clinical Stage of the patient this visit
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<!--This list represents the four WHO clinical stages -->
						<xs:enumeration value="1" />
						<xs:enumeration value="2" />
						<xs:enumeration value="3" />
						<xs:enumeration value="4" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TBStatus" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the TB Status of the patient this visit
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="1" />
						<xs:enumeration value="2" />
						<xs:enumeration value="3" />
						<xs:enumeration value="4" />
						<xs:enumeration value="5" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OtherOIOtherProblems" type="CodeType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Other OIs/ Other
						Problems
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NotedSideEffects" type="CodeType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Noted Side Effects
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ARVDrugRegimen" type="CodedSimpleType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the ARV Regimen of the patient this visit
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ARVDrugAdherence" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the Adherence to ARV Regimen of the patient this
						visit
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="G" />
						<xs:enumeration value="F" />
						<xs:enumeration value="P" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="WhyPoorFairARVDrugAdherence" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						why poor/fair ARV Regimen adherance
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="1" />
						<xs:enumeration value="2" />
						<xs:enumeration value="3" />
						<xs:enumeration value="4" />
						<xs:enumeration value="5" />
						<xs:enumeration value="6" />
						<xs:enumeration value="7" />
						<xs:enumeration value="8" />
						<xs:enumeration value="9" />
						<xs:enumeration value="10" />
						<xs:enumeration value="11" />
						<xs:enumeration value="12" />
						<xs:enumeration value="13" />
						<xs:enumeration value="14" />
						<xs:enumeration value="15" />
						<xs:enumeration value="16" />
						<xs:enumeration value="17" />
						<xs:enumeration value="18" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CotrimoxazoleDose" type="CodedSimpleType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the Cotrimoxazole Dose of the patient this visit
						inclusive of the
						Drug itself
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CotrimoxazoleAdherence" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the Adherence to Cotrimoxazole of the patient
						this visit
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="G" />
						<xs:enumeration value="F" />
						<xs:enumeration value="P" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="WhyPoorFairCotrimoxazoleDrugAdherence"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						why poor/fair Cotrimoxazole Regimen adherance
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="1" />
						<xs:enumeration value="2" />
						<xs:enumeration value="3" />
						<xs:enumeration value="4" />
						<xs:enumeration value="5" />
						<xs:enumeration value="6" />
						<xs:enumeration value="7" />
						<xs:enumeration value="8" />
						<xs:enumeration value="9" />
						<xs:enumeration value="10" />
						<xs:enumeration value="11" />
						<xs:enumeration value="12" />
						<xs:enumeration value="13" />
						<xs:enumeration value="14" />
						<xs:enumeration value="15" />
						<xs:enumeration value="16" />
						<xs:enumeration value="17" />
						<xs:enumeration value="18" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="INHDose" type="CodedSimpleType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the INH Dose of the patient this visit inclusive
						of the Drug itself
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="INHAdherence" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the Adherence to INH of the patient this visit
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="G" />
						<xs:enumeration value="F" />
						<xs:enumeration value="P" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="WhyPoorFairINHDrugAdherence" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						why poor/fair INH Regimen adherance
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="CodeType">
						<xs:enumeration value="1" />
						<xs:enumeration value="2" />
						<xs:enumeration value="3" />
						<xs:enumeration value="4" />
						<xs:enumeration value="5" />
						<xs:enumeration value="6" />
						<xs:enumeration value="7" />
						<xs:enumeration value="8" />
						<xs:enumeration value="9" />
						<xs:enumeration value="10" />
						<xs:enumeration value="11" />
						<xs:enumeration value="12" />
						<xs:enumeration value="13" />
						<xs:enumeration value="14" />
						<xs:enumeration value="15" />
						<xs:enumeration value="16" />
						<xs:enumeration value="17" />
						<xs:enumeration value="18" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="CD4" type="xs:int" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						CD4 absolute value test
						result of the patient this visit
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CD4TestDate" type="xs:date" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Date of CD4 test result
						of the patient this visit
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NextAppointmentDate" type="xs:date"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Date of next
						appointment scheduled for the patient this visit
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="MedicationType">
		<xs:sequence>
			<xs:choice>
				<xs:element name="Drug" type="CodedSimpleType" minOccurs="1"
					maxOccurs="1">
					<xs:annotation>
						<xs:documentation xml:lang="en">
							Code that represents
							the Drug
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="DrugOther" type="StringType" minOccurs="0"
					maxOccurs="1">
					<xs:annotation>
						<xs:documentation xml:lang="en">
							Free text that
							represents the other Drug specified
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="FrequencyCode" type="CodeType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						Code that represents
						the Frequency of the Drug
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Duration" type="StringType" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						The length of time the
						drug should be taken
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="QuantityPrescribed" minOccurs="0"
				maxOccurs="1">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation xml:lang="en">
							The of drugs
							prescribed
						</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="0" />
						<xs:maxInclusive value="50" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="QuantityDispensed" minOccurs="0"
				maxOccurs="1">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation xml:lang="en">
							The number of drugs
							dispensed
						</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:int">
						<xs:minInclusive value="0" />
						<xs:maxInclusive value="50" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SingleDose" type="StringType" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						For Pediatric, the
						single dose
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TotalDailyDose" type="StringType"
				minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="en">
						For Pediatric, the
						total daily dose
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AnswerType">
		<xs:sequence>
			<xs:choice>
				<xs:element name="AnswerCode" type="CodedType" />
				<xs:element name="AnswerDate" type="xs:date" />
				<xs:element name="AnswerDateTime" type="xs:dateTime" />
				<xs:element name="AnswerNumeric" type="NumericType" />
				<xs:element name="AnswerText">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="0" />
							<xs:maxLength value="2000" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CodedType">
		<xs:sequence>
			<xs:element name="Code" type="CodeType" />
			<xs:element name="CodeDescTxt" type="CodeDescTxtType" />
			<xs:element name="CodeSystemCode" type="CodeSystemCodeType" />
			<xs:element name="Text" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0" />
						<xs:maxLength value="255" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CodedSimpleType">
		<xs:sequence>
			<xs:element name="Code" type="CodeType" minOccurs="1" />
			<xs:element name="CodeDescTxt" type="CodeDescTxtType"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="CodeType">
		<xs:restriction base="xs:string">
			<xs:minLength value="0" />
			<xs:maxLength value="50" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodeSystemCodeType">
		<xs:restriction base="xs:string">
			<xs:minLength value="0" />
			<xs:maxLength value="255" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodeDescTxtType">
		<xs:restriction base="xs:string">
			<xs:minLength value="0" />
			<xs:maxLength value="255" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="NumericType">
		<xs:sequence>
			<xs:element name="ComparatorCode" type="ComparatorCodeType"
				minOccurs="0" />
			<xs:element name="Value1">
				<xs:simpleType>
					<xs:restriction base="xs:float" />
				</xs:simpleType>
			</xs:element>
			<xs:element name="SeperatorCode" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0" />
						<xs:maxLength value="10" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Value2" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:float" />
				</xs:simpleType>
			</xs:element>
			<xs:element name="Unit" type="CodedType" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ComparatorCodeType">
		<xs:restriction base="xs:string">
			<xs:minLength value="0" />
			<xs:maxLength value="10" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StringType">
		<xs:restriction base="xs:string">
			<xs:minLength value="0" />
			<xs:maxLength value="255" />
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
